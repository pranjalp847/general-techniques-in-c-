#include <stdio.h>
#include <string.h>

int main()
{
    int i,x,j,sum=0;
    char s[100];
    scanf("%s",s);
    x=strlen(s);
    for(i=48;i<=57;i++)
    {
        sum=0;
        for(j=0;j<x;j++)
        {
            
            if(i==s[j])
            {
                sum++;
            }
            
        }
        printf("%d\n",sum);
    }

    return 0;
}


